{
    "KPackageStructure": "Plasma/Applet",
    "KPlugin": {
        "Authors": [
            {
                "Name": "se7uh"
            }
        ],
        "Category": "Utilities",
        "Description": "Quick note widget for KDE Plasma",
        "Icon": "accessories-text-editor",
        "Id": "com.se7uh.kquicknote",
        "License": "MIT",
        "Name": "KQuickNote",
        "Version": "1.0",
        "Website": "https://github.com/se7uh/kquicknote",
